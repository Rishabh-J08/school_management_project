{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "DB_HOST": "@db_host",
      "DB_USER": "@db_user", 
      "DB_PASSWORD": "@db_password",
      "DB_NAME": "@db_name"
    },
    "regions": ["all"],
    "framework": "express"
  }